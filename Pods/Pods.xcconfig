ALWAYS_SEARCH_USER_PATHS = YES
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = ${PODS_HEADERS_SEARCH_PATHS}
OTHER_LDFLAGS = -ObjC
PODS_BUILD_HEADERS_SEARCH_PATHS = "${PODS_ROOT}/BuildHeaders" "${PODS_ROOT}/BuildHeaders/ECSlidingViewController" "${PODS_ROOT}/BuildHeaders/PunchScrollView" "${PODS_ROOT}/BuildHeaders/SBJson"
PODS_HEADERS_SEARCH_PATHS = ${PODS_PUBLIC_HEADERS_SEARCH_PATHS}
PODS_PUBLIC_HEADERS_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/ECSlidingViewController" "${PODS_ROOT}/Headers/PunchScrollView" "${PODS_ROOT}/Headers/SBJson"
PODS_ROOT = ${SRCROOT}/Pods